- var theme_customizer      = true;
- var counter               = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var scrollbar             = true;
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid
            .page-title
              .row
                .col-sm-6.ps-0
                  h3
                    | Pricing

                .col-sm-6.pe-0
                  ol.breadcrumb
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Ecommerce
                    li.breadcrumb-item.active Pricing
          // Container-fluid starts
          .container-fluid
            .row
              .col-sm-12
                .card
                  .card-header.pb-0 
                    h4 Become member
                  .card-body.pricing-content
                    .row.pricing-col.gy-4
                      .col-lg-3.col-sm-6.box-col-3
                        .pricingtable
                          .pricingtable-header
                            h3.title Standard
                          .price-value
                            span.currency $
                            span.amount 10
                            span.duration /mo
                          ul.pricing-content
                            li 50GB Disk Space
                            li 50 Email Accounts
                            li Maintenance
                            li 15 Subdomains
                          .pricingtable-signup
                            a.btn.btn-primary.btn-lg(href='#') Sign Up
                      .col-lg-3.col-sm-6.box-col-3
                        .pricingtable
                          .pricingtable-header
                            h3.title Premium
                          .price-value
                            span.currency $
                            span.amount 20
                            span.duration /mo
                          ul.pricing-content
                            li 10% on all product
                            li 50 Email Accounts
                            li Maintenance
                            li 15 Subdomains
                          .pricingtable-signup
                            a.btn.btn-primary.btn-lg(href='#') Sign Up
                      .col-lg-3.col-sm-6.box-col-3
                        .pricingtable
                          .pricingtable-header
                            h3.title Auther pack
                          .price-value
                            span.currency $
                            span.amount 50
                            span.duration /mo
                          ul.pricing-content
                            li Upload 50 product
                            li 50 Email Accounts
                            li Maintenance
                            li 15 Subdomains
                          .pricingtable-signup
                            a.btn.btn-primary.btn-lg(href='#') Sign Up
                      .col-lg-3.col-sm-6.box-col-3
                        .pricingtable
                          .pricingtable-header
                            h3.title Auther pack
                          .price-value
                            span.currency $
                            span.amount 50
                            span.duration /mo
                          ul.pricing-content
                            li Upload 50 product
                            li 50 Email Accounts
                            li Maintenance
                            li 15 Subdomains
                          .pricingtable-signup
                            a.btn.btn-primary.btn-lg(href='#') Sign Up
                .card
                  .card-header.pb-0 
                    h4 Simple Pricing Card
                  .card-body.pricing-content
                    .row.g-sm-4.g-3
                      .col-lg-3.col-sm-6.box-col-3
                        .card.text-center.pricing-simple
                          .card-body
                            h3 Standard
                            h2 $15
                            h3.mb-0 Standard Plan
                          a.btn.btn-lg.btn-primary.btn-block(href='#') Purchase
                      .col-lg-3.col-sm-6.box-col-3
                        .card.text-center.pricing-simple
                          .card-body
                            h3 Business
                            h2 $25
                            h3.mb-0 Business Plan
                          a.btn.btn-lg.btn-primary.btn-block(href='#') Purchase
                      .col-lg-3.col-sm-6.box-col-3
                        .card.text-center.pricing-simple
                          .card-body
                            h3 Premium
                            h2 $35
                            h3.mb-0 Premium Plan
                          a.btn.btn-lg.btn-primary.btn-block(href='#') Purchase
                      .col-lg-3.col-sm-6.box-col-3
                        .card.text-center.pricing-simple
                          .card-body
                            h3 Extra
                            h2 $45
                            h3.mb-0 Extra Plan
                          a.btn.btn-lg.btn-primary.btn-block(href='#') Purchase
        include ../../components/footer
    include ../../components/footer-files
